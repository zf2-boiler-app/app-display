{
    "name": "zf2-boiler-app/app-display",
    "licence": "MIT",
    "type": "library",
    "description": "ZF2 BoilerApp Display module",
    "keywords": ["Zend Framework 2","ZF2 Boiler-App","templating","Twitter bootstrap"],
    "authors": [
        {
            "name": "Neilime",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/zf2-boiler-app/app-display/issues"
    },
    "minimum-stability" : "dev",
    "require": {
        "php": ">=5.3.3",
        "ext-intl": "*",
        "zendframework/zendframework": "dev-master",
        "neilime/zf2-assets-bundle": "dev-master",
        "neilime/zf2-twb-bundle": "dev-master",
        "neilime/zf2-tree-layout-stack": "dev-master",
       	"twitter/bootstrap": "dev-master",
        "fortawesome/font-awesome": "dev-master",
        "fabiomcosta/mootools-meio-mask": "*",
        "arian/iFrameFormRequest": "*",
       	"nak5ive/Form.PasswordStrength": "*"
    },
    "require-dev":{
    	"zf2-boiler-app/app-test": "dev-master",
    	"neilime/zf2-translator-tools": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "BoilerAppDisplay": "src/"
        },
        "classmap" : ["Module.php"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "process-timeout": 3000
    }
}